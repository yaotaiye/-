.mv-form-group {
	margin-bottom: 110rpx
}

.mv-form-label {
	display: inline-block;
	margin-top: 0;
	margin-bottom: 0;
	padding-top: .8em;
	text-align: right;
	font-size: 32rpx
}

.mv-input-group {
	position: relative;
	display: table;
	border-collapse: separate
}

.mv-form-group:after,.mv-form-group:before {
	content: "";
	display: table
}

.mv-form-group:after {
	clear: both
}

.mv-input-group-label {
	display: table-cell;
	width: 1%;
	white-space: nowrap;
	vertical-align: middle;
	padding: 0 1em;
	font-size: 32rpx;
	font-weight: 400;
	color: #666;
	text-align: center;
	border-radius: 0
}

.mv-form .form-input,.mv-form .form-select,.mv-form .form-textarea,.mv-form-field {
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	display: block;
	width: 100%;
	padding: .8em 1em;
	font-size: 32rpx;
	line-height: 1.2;
	color: #666;
	vertical-align: middle;
	background-color: #fff;
	border: 1px solid #ccc;
	-webkit-appearance: none;
	-webkit-transition: border-color .15s ease-in-out,-webkit-box-shadow .15s ease-in-out;
	transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out
}

.mv-form .form-input.xs {
